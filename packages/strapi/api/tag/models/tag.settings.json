{
  "connection": "default",
  "collectionName": "tags",
  "info": {
    "name": "Tag"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Tag": {
      "type": "string"
    },
    "quotes": {
      "via": "tags",
      "collection": "quote",
      "dominant": true
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
